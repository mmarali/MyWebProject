<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Sylabus</title>
    <link rel="stylesheet" href="~/frontend/dist/css/app.css" />
    <link rel="stylesheet" href="~/frontend/dist/css/chunk-vendors.css" />
</head>
<body>

<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

@Html.Partial("_Footer")

@await RenderSectionAsync("Scripts", required: false)
<script src="~/frontend/dist/js/chunk-vendors.js"></script>
<script src="~/frontend/dist/js/app.js"></script>

</body>
</html>